<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5"
    tools:context=".ui.mypage.helpFragment">

    <RelativeLayout
        android:id="@+id/relativeLayout7"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:background="#AB715D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/txt_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/img_help_back"
            android:gravity="center_horizontal|center_vertical"
            android:paddingRight="20dp"
            android:text="帮助与反馈"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText,RtlHardcoded,RtlSymmetry" />

        <ImageView
            android:id="@+id/img_help_back"
            android:layout_width="37dp"
            android:layout_height="37dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="14dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:clickable="true"
            android:src="@drawable/selector_backs"
            tools:ignore="ContentDescription,ImageContrastCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout13"
        android:layout_width="0dp"
        android:layout_height="215dp"
        android:background="@drawable/yunback"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout7">

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="2dp"
            android:layout_marginEnd="-2dp"
            android:layout_marginBottom="18dp"
            android:fontFamily="sans-serif-condensed"
            android:gravity="center"
            android:text="&quot;HI&quot;值得信任"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            tools:ignore="HardcodedText" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="128dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="60dp"
            android:ems="1"
            android:text="万般皆下品"
            android:textColor="?android:attr/colorActivatedHighlight"
            android:textStyle="bold|italic"
            tools:ignore="HardcodedText,TextContrastCheck" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="60dp"
            android:layout_toStartOf="@+id/textView19"
            android:ems="1"
            android:text="唯有读书高"
            android:textColor="@android:color/holo_purple"
            android:textStyle="bold|italic"
            tools:ignore="HardcodedText,TextContrastCheck,TextContrastCheck" />
    </RelativeLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout13">

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            android:layout_marginTop="16dp"
            android:text="帮助"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView16">

            <TextView
                android:id="@+id/txt_help_read"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="阅读记忆法"
                android:textColor="@drawable/selector_help"
                android:textSize="16sp"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />

            <TextView
                android:id="@+id/txt_help_book"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="笔记记忆法"
                android:textColor="@drawable/selector_help"
                android:textSize="16sp"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />

            <TextView
                android:id="@+id/txt_help_jian"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="间隔反复记忆法"
                android:textColor="@drawable/selector_help"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />

            <TextView
                android:id="@+id/txt_help_lian"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="“勤”即勤于查词典“联”即联想记忆法"
                android:textColor="@drawable/selector_help"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />

            <TextView
                android:id="@+id/txt_help_qin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="“勤”即勤于查词典"
                android:textColor="@drawable/selector_help"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            android:layout_marginTop="16dp"
            android:text="反馈"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
            tools:ignore="HardcodedText,RtlHardcoded" />

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView23">

            <TextView
                android:id="@+id/txt_help_account"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="个人账号问题"
                android:textColor="@drawable/selector_kui"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />

            <TextView
                android:id="@+id/txt_help_gong"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="功能相关问题"
                android:textColor="@drawable/selector_kui"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />

            <TextView
                android:id="@+id/txt_help_learn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                android:text="学习相关问题"
                android:textColor="@drawable/selector_kui"
                tools:ignore="HardcodedText,RtlHardcoded,TextContrastCheck" />
        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>